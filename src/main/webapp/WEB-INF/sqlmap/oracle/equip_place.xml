	<insert id="merge">
	INSERT INTO ABILITIES (
		ABILITY_NAME 
		,ABILITY_SEQ
		)
		values(
		#{ABILITY_NAME}
		,#{ABILITY_SEQ}
		)
		ON DUPLICATE KEY UPDATE 
		ABILITY_NAME = #{ABILITY_NAME}
		,ABILITY_SEQ = #{ABILITY_SEQ}
			
	</insert>
